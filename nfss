local page = venyx:addPage("AutoFarm", 5012544693)
local section1 = page:addSection("AutoFarming")

section1:addDropdown("Select Area", {"Area1", "Area2", "Area3", "Area4", "Area5"}, function(cas)
    getgenv().autofarm_area = tostring(cas)
end)

section1:addDropdown("Select Boss", {1,2,3,4,5}, function(cas)
    getgenv().autofarm_boss = tonumber(cas)
end)

section1:addToggle("Start Auto Farm", nil, function(value)
    getgenv().autofarm = value
    while getgenv().autofarm and wait(0.525) do
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("FightOver"):FireServer(getgenv().autofarm_area, getgenv().autofarm_boss)
    end
end)

local section1 = page:addSection("Boss Farm")

section1:addDropdown("Select Boss", {"Area1", "Area2"}, function(cas)
    getgenv().boss = tostring(cas)
end)

section1:addToggle("Farm Boss", nil, function(value)
    getgenv().farmboss = value
    while getgenv().farmboss and wait(0.525) do
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("FightOver"):FireServer(getgenv().boss, 0)
    end
end)

section1:addButton("Teleport To Selected Boss", function()
    if getgenv().boss == "Area1" then
        local player = game.Players.LocalPlayer
        local targetPosition = game.workspace.Fighters.LoneWanderer.Head.Position
        player.Character:MoveTo(targetPosition)
    elseif getgenv().boss == "Area2" then
        local player = game.Players.LocalPlayer
        local targetPosition = game.workspace.Fighters.ShadowAssassin.Head.Position
        player.Character:MoveTo(targetPosition)
    end
end)

