local uiname = "Louv Hub - Venyx2.21"

if game.CoreGui:FindFirstChild(uiname) then
    game.CoreGui[uiname]:Destroy()
    getgenv().auto_katana = false
    getgenv().auto_claim = false
    getgenv().craft = false
    getgenv().spinner = false
    getgenv().wwwx = 1500
    
    getgenv().x = 0
    getgenv().y = 0
    getgenv().z = 2
    
    getgenv().dummy = false
    getgenv().dummy_bag = 4
    getgenv().area_dummy = "Area5"
    
    getgenv().power_booth = false
    getgenv().katana_n = 1
    getgenv().nunchuck_n = 1
    
    getgenv().punches = false
    getgenv().punch_bag = 7
    getgenv().area_punch = "Area5"
    
    getgenv().auto_equip = false
    getgenv().weapon = "Katana"
    getgenv().weapon_area = "Area5"
    getgenv().weapon_numb = 1
    
    getgenv().auto_hatch = false
    getgenv().egg_speed = 0
    getgenv().egg_amount = 1
    getgenv().egg = "Starter Egg"
    getgenv().Animation = true
    
    getgenv().autofarm = false
    getgenv().autofarm_area = "Area1"
    getgenv().autofarm_boss = 1
    
    getgenv().farmboss = false
    getgenv().boss = "Area1"
    
    getgenv().auto_rebirth = false
    
    getgenv().unequip = false
    getgenv().equip = false
end

local uiname = "Louv Hub - Venyx2.21"


local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/zxciaz/VenyxUI/main/Reuploaded"))() --someone reuploaded it so I put it in place of the original back up so guy can get free credit.
local venyx = library.new(uiname, 5013109572)

local x = 511
local y = 428

local b1 = x-36
local b2 = x-76

local spnz = false
local latest = ""

local button2 = Instance.new("TextButton")
button2.Text = "-"
button2.TextSize = 16
button2.Size = UDim2.new(0, 32, 0, 32)
button2.Position = UDim2.new(0, b2, 0, 2)
button2.BackgroundTransparency = 0.5
button2.BackgroundColor3 = Color3.new(0, 0, 0)
button2.BorderSizePixel = 0
button2.Parent = game:GetService("CoreGui")[uiname].Main.TopBar
button2.TextColor3 = Color3.new(255, 255, 255)
button2.ZIndex = 5
button2.TextStrokeTransparency = 0

local uiCorner2 = Instance.new("UICorner")
uiCorner2.CornerRadius = UDim.new(100, 100)
uiCorner2.Parent = button2

local button1 = Instance.new("TextButton")
button1.Text = "X"
button1.TextSize = 12
button1.Size = UDim2.new(0, 32, 0, 32)
button1.Position = UDim2.new(0, b1, 0, 2)
button1.BackgroundTransparency = 0.5
button1.BackgroundColor3 = Color3.new(0, 0, 0)
button1.BorderSizePixel = 0
