local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/zxciaz/VenyxUI/main/Reuploaded"))() --someone reuploaded it so I put it in place of the original back up so guy can get free credit.
local venyx = library.new("Louv Hub", 5013109572)

getgenv().auto_katana = false
getgenv().auto_claim = false
getgenv().craft = false
getgenv().spinner = false

-- first page
local page = venyx:addPage("Main", 5012544693)
local section1 = page:addSection("Gifts /-/ Free")

section1:addToggle("Auto Claim Gifts", nil, function(value)
    getgenv().auto_claim = value
    while getgenv().auto_claim and wait() do
        loadstring(game:HttpGet("https://pastebin.com/raw/mdaf0fHD"))()
    end
end)

section1:addToggle("Auto Spin Wheel", nil, function(value)
    getgenv().spinner = value
    while getgenv().spinner and wait() do
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("Wheel"):InvokeServer("Spin")
    end
end)

section1:addButton("Redeem Codes", function()
    local codes = {"RELEASE", "UPDATE","chaos","SORRY","UGC","MISSING","GamingDan","SECRET"}
    for _, code in pairs(codes) do
        game:GetService("ReplicatedStorage"):WaitForChild("KingermanCodeSystem"):WaitForChild("Redeem"):InvokeServer(tostring(code))
        wait(0.05)
    end
end)

local section1 = page:addSection("Unlock")

section1:addButton("Unlock Area 2", function()
    game.workspace.Zones.TeleportToArea2.Wins.Value = 0
end)
section1:addButton("Unlock Area 3", function()
    game.workspace.Zones.TeleportToArea3.Wins.Value = 0
end)
section1:addButton("Unlock Area 4", function()
    game.workspace.Zones.TeleportToArea4.Wins.Value = 0
end)
section1:addButton("Unlock Area 5", function()
    game.workspace.Zones.TeleportToArea5.Wins.Value = 0
end)

local section1 = page:addSection("Teleport")
section1:addButton("Teleport To Area 1", function()
    local player = game.Players.LocalPlayer
    local targetPosition = game.workspace.Zones.TeleportBackToDojo.Zone.Position
    player.Character:MoveTo(targetPosition)
end)
section1:addButton("Teleport To Area 2", function()
    local player = game.Players.LocalPlayer
    local targetPosition = game.workspace.Zones.TeleportToArea2.Zone.Position
    player.Character:MoveTo(targetPosition)
end)
section1:addButton("Teleport To Area 3", function()
    local player = game.Players.LocalPlayer
    local targetPosition = game.workspace.Zones.TeleportToArea3.Zone.Position
    player.Character:MoveTo(targetPosition)
end)
section1:addButton("Teleport To Area 4", function()
    local player = game.Players.LocalPlayer
    local targetPosition = game.workspace.Zones.TeleportToArea4.Zone.Position
    player.Character:MoveTo(targetPosition)
end)
section1:addButton("Teleport To Area 5", function()
    local player = game.Players.LocalPlayer
    local targetPosition = game.workspace.maps:GetChildren()[12].Model:GetChildren()[9].Position
    targetPosition = targetPosition + Vector3.new(5, 0, 5)
    player.Character:MoveTo(targetPosition)    
end)

local page = venyx:addPage("Power", 5012544693)
local section1 = page:addSection("Katana & NunChucks")

section1:addToggle("Auto Katana/Nunchucks Power", nil, function(value)
    getgenv().auto_katana = value
    while getgenv().auto_katana and wait() do
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("DoAction"):FireServer()
    end
end)

--local args = {[1] = "Area4",[2] = "Dummies",[3] = 1}
--game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("DoAction"):FireServer(unpack(args))
local section1 = page:addSection("Dummies/Bags Settings")

getgenv().x = -4
getgenv().y = 0
getgenv().z = 2

section1:addSlider("Bags: X", -10, -4, 10, function(value)
    getgenv().x = value
end)
section1:addSlider("Bags: Y", -10, 0, 10, function(value)
    getgenv().y = value
end)
section1:addSlider("Bags: Z", -10, 2, 10, function(value)
    getgenv().z = value
end)

local section1 = page:addSection("NunChuck Dummies")

getgenv().dummy = true
getgenv().dummy_bag = 4
getgenv().area_dummy = "Area5"

section1:addDropdown("Area", {"Area1", "Area2", "Area3", "Area4", "Area5"}, function(xed)
    getgenv().area_dummy = xed
end)

section1:addDropdown("Bag", {1,2,3,4}, function(xed)
    getgenv().dummy_bag = xed
end)

section1:addToggle("Most Punch Power", nil, function(value)
    getgenv().dummy = value
    while getgenv().dummy and wait() do
        local player = game.Players.LocalPlayer
        local character = player.Character
        character:SetPrimaryPartCFrame(CFrame.new(workspace.Dummies[getgenv().area_dummy][getgenv().dummy_bag]["Root"].Position) + Vector3.new(getgenv().x, getgenv().y, getgenv().z))
        local args = {[1] = getgenv().area_dummy,[2] = "Dummies",[3] = getgenv().dummy_bag}
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("DoAction"):FireServer(unpack(args))
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("DoAction"):FireServer()
    end
end)

local section1 = page:addSection("Punch Bags")

getgenv().punches = true
getgenv().punch_bag = 7
getgenv().area_punch = "Area5"

section1:addDropdown("Area", {"Area1", "Area2", "Area3", "Area4", "Area5"}, function(xed)
    getgenv().area_punch = xed
end)

section1:addDropdown("Bag", {1,2,3,4,5,6,7}, function(xed)
    getgenv().punch_bag = xed
end)

section1:addToggle("Most Punch Power", nil, function(value)
    getgenv().punches = value
    while getgenv().punches and wait() do
        local player = game.Players.LocalPlayer
        local character = player.Character
        if getgenv().punch_bag == 7 then
            character:SetPrimaryPartCFrame(CFrame.new(workspace.PunchingBags[getgenv().area_punch].VIP["Root"].Position) + Vector3.new(getgenv().x, getgenv().y, getgenv().z))
        else
            character:SetPrimaryPartCFrame(CFrame.new(workspace.PunchingBags[getgenv().area_punch][getgenv().punch_bag]["Root"].Position))
        end
        local args = {[1] = getgenv().area_punch,[2] = "PunchingBag",[3] = getgenv().punch_bag}
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("DoAction"):FireServer(unpack(args))
    end
end)

local page = venyx:addPage("Equiper", 5012544693)
local section1 = page:addSection("Equip-Script")

getgenv().auto_equip = false
getgenv().weapon = "Katana"
getgenv().weapon_area = "Area5"
getgenv().weapon_numb = 1

-- game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("EquipAndUnequipTool"):FireServer(getgenv().weapon_area, getgenv().weapon, getgenv().weapon_numb)

