    local player = game.Players.LocalPlayer
    local targetPosition = game.workspace.Zones.TeleportToArea4.Zone.Position
    player.Character:MoveTo(targetPosition)
end)
section1:addButton("Teleport To Area 5", function()
    local player = game.Players.LocalPlayer
    local targetPosition = game.workspace.maps:GetChildren()[12].Model:GetChildren()[9].Position
    targetPosition = targetPosition + Vector3.new(5, 0, 5)
    player.Character:MoveTo(targetPosition)    
end)
local page = venyx:addPage("Power", 5012544693)
local section1 = page:addSection("Katana & NunChucks")

section1:addToggle("Auto Katana/Nunchucks Power (Fast)", nil, function(value)
    getgenv().auto_katana = value
    while getgenv().auto_katana and wait() do
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("DoAction"):FireServer()
    end
end)

section1:addSlider("Wait Before Change (MS)", 1, 0, 10000, function(value)
    getgenv().wwwx = value / 10000
end)

section1:addToggle("AutoPower (Booth, Slow)", nil, function(value)
    getgenv().power_booth = value
    while getgenv().power_booth and wait() do
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("EquipAndUnequipTool"):FireServer("Area5", "Katana", getgenv().katana_n)
        wait(getgenv().wwwx)
        game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("EquipAndUnequipTool"):FireServer("Area5", "Nunchuck", getgenv().nunchuck_n)
        wait(getgenv().wwwx)
    end
end)

