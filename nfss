button1.TextColor3 = Color3.new(255, 255, 255)
button1.ZIndex = 5
button1.TextStrokeTransparency = 0

local uiCorner1 = Instance.new("UICorner")
uiCorner1.CornerRadius = UDim.new(100, 100)
uiCorner1.Parent = button1

button2.MouseButton1Click:Connect(function()
    spnz = not spnz
    if spnz == true then
	    Game.CoreGui[uiname].Main.Size = UDim2.new(0, x, 0, 40)
        local main = Game.CoreGui[uiname].Main
        local sections = {"Auto Rebirth", "AutoFarm", "Main", "Power", "Equiper", "Eggs", "Settings"}
        for _, sectionName in ipairs(sections) do
            if main[sectionName].Visible then
                latest = sectionName
                break
            end
        end
        Game.CoreGui[uiname].Main["Auto Rebirth"].Visible = false
        Game.CoreGui[uiname].Main["AutoFarm"].Visible = false
        Game.CoreGui[uiname].Main["Main"].Visible = false
        Game.CoreGui[uiname].Main["Power"].Visible = false
        Game.CoreGui[uiname].Main["Equiper"].Visible = false
        Game.CoreGui[uiname].Main["Eggs"].Visible = false
        Game.CoreGui[uiname].Main["Settings"].Visible = false
    else
        Game.CoreGui[uiname].Main.Size = UDim2.new(0, x, 0, y)
        Game.CoreGui[uiname].Main[latest].Visible = true
    end
end)
button2.MouseButton2Click:Connect(function()
end)
