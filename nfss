    while getgenv().craft and wait() do
        game:GetService("ReplicatedStorage"):WaitForChild("PetSystem"):WaitForChild("PetInventoryHandler"):FireServer("CraftAll")
    end
end)

section1:addToggle("Auto Hatch", nil, function(value)
    getgenv().auto_hatch = value
    while getgenv().auto_hatch and wait(getgenv().egg_speed) do
        if egg == "EggEvent1" then
            game:GetService("ReplicatedStorage"):WaitForChild("Remotes"):WaitForChild("EggEvent1"):FireServer(getgenv().egg_amount)
        else
            game:GetService("ReplicatedStorage"):WaitForChild("PetSystem"):WaitForChild("PurchaseEgg"):FireServer(getgenv().egg, getgenv().egg_amount)
